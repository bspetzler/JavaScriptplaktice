function isDivisible(divisee, divisor) {
  return divisee % divisor === 0;

  // your code here
}

const six = 6;
const three = 3;
poo = isDivisible(six, three)
console.log(poo)
/* From here down, you are not expected to 
   understand.... for now :)  
   
   
